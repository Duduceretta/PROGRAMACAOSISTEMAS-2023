      "source": [
        "word = str(input(\"Insira uma palavra -> \"))\n",
        "\n",
        "inverseword = word[::-1]\n",
        "\n",
        "print(f\"\\n{word}\")\n",
        "print(f\"{inverseword}\\n\")\n",
        "\n",
        "if word == inverseword:\n",
        "  print(\"A palavra informada é palíndromo!\")\n",
        "else:\n",
        "  print(\"A palavra não é palíndromo...\")"
      ],
